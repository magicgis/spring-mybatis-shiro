<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <settings>
      <setting name="cacheEnabled" value="true"/>
      <setting name="defaultExecutorType" value="BATCH"/>
  </settings>
  
  <typeAliases>
  	 <!-- 用户 -->
     <typeAlias type="cn.tbj.entity.main.SecurityUserPO" alias="SecurityUserPO"/>
     <!-- 角色 -->
     <typeAlias type="cn.tbj.entity.main.SecurityRolePO" alias="SecurityRolePO"/>
     <!-- 用户角色 -->
     <typeAlias type="cn.tbj.entity.main.SecurityUserRolePO" alias="SecurityUserRolePO"/>
     <!-- 用户角色权限 -->
     <typeAlias type="cn.tbj.entity.main.SecurityRolePermission" alias="SecurityRolePermission"/>
     <!--  权限模块 -->
     <typeAlias type="cn.tbj.entity.main.SecurityModule" alias="SecurityModule"/>
  </typeAliases>

</configuration>